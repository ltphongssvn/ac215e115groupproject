# File: .gitignore (project root)

# IDE specific files
.idea/
*.iml
.vscode/
*.swp
*.swo

# OS specific files
.DS_Store
Thumbs.db

# Temporary files
*.tmp
*.temp
*.log

# Node.js dependencies (if you add any later)
node_modules/
package-lock.json
yarn.lock

# Python files (if you add Python components later)
__pycache__/
*.py[cod]
.venv/
venv/

# Environment variables
.env
.env.local

# Build outputs
dist/
build/
*.min.js
*.min.css

# Test environments
test_*env/
test_venv/
test_uv_env/

# Conda environments
test_ac215/

# Additional Python patterns
*.egg-info/
pip-log.txt
.coverage
.pytest_cache/
*.ipynb_checkpoints/

# Project-specific exclusions
backups/
test-docker-service/

# Data files (never commit large datasets)
data/integrated/
data/processed/
data/raw/

# Environment files with secrets
.env.*
!.env.example
!.env.*.examplebackups/
