# GCP Cloud SQL Configuration Template for Rice Market AI System

# AirTable Settings
AIRTABLE_API_KEY=your_airtable_api_key_here
AIRTABLE_BASE_ID=your_base_id_here

# GCP Cloud SQL Connection
POSTGRES_HOST=your_cloud_sql_ip
POSTGRES_PORT=5432
POSTGRES_DATABASE=rice_market_db
POSTGRES_USER=rice_admin
POSTGRES_PASSWORD=your_password_here
POSTGRES_SCHEMA=airtable_sync

# Sync Configuration
SYNC_MODE=incremental
RATE_LIMIT_DELAY=0.25

# GCP Project Info
GCP_PROJECT=your_project_id
GCP_INSTANCE=your_instance_name
GCP_REGION=us-central1
